@import "/static/css/vendor.wxss";

page {
  height: 100%;
  width: 100%;
}
._button {
  height: 100%;
  width: 100%;
}
.counter-warp {
  width: 100%;
  height: 100%;
  padding-top: 20rpx;
  position: relative;
  font-size: 24rpx;
  background-color: #f1f0f5;
  overflow-y: scroll;
  padding-bottom: 180rpx;
}
.counter-warp .details {
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 20rpx;
  width: 96%;
  margin: 0 auto;
  border-radius: 10rpx;
  background-color: white;
}
.detailMess {
  min-height: 150rpx;
  padding: 0 8rpx;
}
.detailMess ._div:nth-child(2) {
  margin-top: 16rpx;
  color: #666666;
}
.detailMess .title ._span:nth-child(1) {
  font-size: 30rpx;
  font-weight: 600;
  color: black;
}
.detailMess .title ._span:nth-child(2) {
  height: 30rpx;
  line-height: 30rpx;
  float: right;
  color: red;
}
.imgLishi {
  margin-right: 10rpx;
  display: block;
  height: 26rpx;
  width: 26rpx;
  float: right;
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.detailMoney {
  box-sizing: border-box;
  padding-top: 20rpx;
  height: 110rpx;
  border-top: 2rpx solid #e5e5e5;
}
.detailMoney>._div {
  box-sizing: border-box;
  width: 50%;
  float: left;
}
.sail {
  border-right: 2rpx solid #f3f3f3;
}
.detailMoney>._div>._span {
  display: block;
  width: 100%;
  text-align: center;
  color: #666666;
}
.detailMoney>._div>._span:nth-child(3) {
  margin-top: 16rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: black;
}
.ball {
  margin-top: 22rpx;
  margin-bottom: 18rpx;
  display: block;
  height: 70rpx;
}
.ball>._span {
  margin-right: 10rpx;
  color: white;
  height: 70rpx;
  width: 70rpx;
  display: block;
  float: left;
  border-radius: 50%;
  text-align: center;
  line-height: 70rpx;
}
.ball .blueBall {
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100%;
}
.ball .redBall {
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100%;
}
.rules {
  margin-top: 26rpx;
  min-height: 400rpx;
}
.rules ._div:nth-child(2n+1) {
  background-color: white;
}
.rules>._div {
  min-height: 82rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.rules .top {
  color: #666666;
  height: 86rpx;
  line-height: 86rpx;
}
.rules>._div>._span {
  padding: 6rpx 0;
  flex: 1;
}
.rules .top ._span {
  display: block;
  height: 86rpx;
}
.orderShare {
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  height: 150rpx;
  width: 100%;
  padding: 38rpx 40rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.orderShare ._div {
  float: left;
  height: 70rpx;
  min-width: 40%;
  border-radius: 10rpx;
  color: white;
  font-size: 32rpx;
  font-weight: 600;
  text-align: center;
  line-height: 70rpx;
}
.orderShare .order {
  background-color: #fe5d38;
}
.orderShare .order1 {
  background-color: #c1c1c1;
}
.orderShare .share {
  background-color: #ffa060;
}
._button {
  height: 70rpx;
  border: none;
  line-height: 70rpx;
  font-size: 32rpx;
  background-color: #ffa060;
  width: 100%;
  color: white;
}
._button::after {
  border: 0;
}
.confirmShow {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.4);
  z-index: 10;
}
.confirmBtn {
  height: 472rpx;
  width: 494rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  background-color: white;
  box-sizing: border-box;
  padding: 100rpx;
  border-radius: 10rpx;
}
.confirmBtn ._div ._span:nth-child(1) {
  display: block;
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100%;
  margin: 0 auto;
}
.confirmBtn ._div ._span:nth-child(2) {
  margin-top: 20rpx;
  display: block;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: black;
}
.confirmBtn ._div:nth-child(2) {
  height: 80rpx;
  width: 300rpx;
  border-radius: 10rpx;
  background-color: #fb634f;
  color: white;
  font-size: 32rpx;
  text-align: center;
  line-height: 80rpx;
  margin-top: 72rpx;
}